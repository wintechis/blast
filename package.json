{
  "name": "blast",
  "version": "0.0.1",
  "description": "Block Applications for Things",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wintechis/blast.git"
  },
  "bugs": {
    "url": "https://github.com/wintechis/blast/issues"
  },
  "homepage": "https://github.com/wintechis/blast#readme",
  "author": "Thomas Wehr",
  "engines": {
    "node": ">=12.11.0"
  },
  "scripts": {
    "build": "yarn run build:core && yarn run build:web",
    "build:core": "yarn run clean && tsc && yarn run copy",
    "build:web": "cd examples/web/ && yarn install && yarn run build",
    "serve": "yarn run build && cd examples/web/ && yarn run serve",
    "copy": "copyfiles -u 1 \"src/**/*.js\" \"src/**/*.json\" \"src/**/*.bin\" dist/",
    "lint": "eslint \"src/**/**.js\"",
    "clean": "yarn rimraf dist",
    "fix": "eslint --fix \"src/**/**.js\"",
    "test": "mocha --loader=esmock \"test/**/*.mocha.js\" -u tdd"
  },
  "type": "module",
  "main": "index.html",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@blockly/dev-tools": "3.1.3",
    "@types/node": "^14.11.2",
    "@types/web-bluetooth": "^0.0.14",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esmock": "^1.7.4",
    "js-interpreter": "https://github.com/derwehr/js-interpreter.git",
    "jsdoc": "^3.6.7",
    "mocha": "^9.2.2",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@elgato-stream-deck/webhid": "^5.2.0",
    "@inrupt/solid-client": "^1.19.0",
    "@node-wot/core": "0.8.1",
    "@node-wot/td-tools": "0.8.1",
    "@tensorflow/tfjs": "^3.18.0",
    "babel-polyfill": "^6.26.0",
    "blockly": "8.0.2",
    "express": "^4.18.1",
    "file-saver": "^2.0.5",
    "joy-con-webhid": "^0.2.0",
    "rxjs": "5.5.11",
    "typescript": "^4.7.4",
    "urdf": "https://github.com/derwehr/uRDF.js.git",
    "webpack": "^5.70.0",
    "wot-typescript-definitions": "0.8.0-SNAPSHOT.23"
  },
  "packageManager": "yarn@3.2.0"
}
