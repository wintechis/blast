{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "module": "esnext",
    "lib": [
        "es6",
        "dom"
    ],
    "esModuleInterop": true,
    "target": "ES2018",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [],
  "target": "ES2018",
}