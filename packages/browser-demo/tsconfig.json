{
  "extends": "../../tsconfig.json",
  "include": ["src/**/*"],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"],
    },
  },
}
